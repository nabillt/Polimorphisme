package polimorphisme;

/**
 *
 * @author LENOVO
 */
public class Direktur extends Pegawai {

    /**
     * @param args the command line arguments
     */
    
    private static final int gajiDir=100000;
    private static final int tunjangan=50000;
    
    @Override
    public int gaji(){
        return gajiDir;
    }
    public int tunjangan(){
        return tunjangan;
    }
}
